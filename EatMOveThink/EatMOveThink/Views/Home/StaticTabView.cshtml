
<style>
    strong,b, h3, h5 {
        text-shadow: 1px 1px 1px #ccc;
    }
              
    nav{
        background-color:#71b083 !important
    }
</style>
<section class="single-page-title" style="padding: 56% 0 5%;background:url(@ViewBag.Image) no-repeat bottom;">
    <div class="container text-center">
        @if (ViewBag.Title.ToLower() == "whatsnew")
        {
            <h2>WHAT’S NEW</h2>
        }
        else
        {
            <h2>@ViewBag.Title</h2>
        }
    </div>
</section>
<!-- .page-title -->

<section class="x-services ptb-100 gray-bg">
    <section class="section-title">
        <div class="container text-center">
            @if (ViewBag.Title.ToLower() == "whatsnew")
            {
                <h2>
                    WHAT’S NEW Detail &nbsp; &nbsp; &nbsp;
                    @if (Session["admin"] != null)
                    {
                        <span><a href="@(ViewBag.Title + "/Editor")"><i class="fa fa-edit"></i></a></span>
                    }
                </h2>
            }
            else
            {
                <h2>
                    @ViewBag.Title Programs  &nbsp; &nbsp; &nbsp;
                    @if (Session["admin"] != null)
                    {
                        <span><a href="@(ViewBag.Title + "/Editor")"><i class="fa fa-edit"></i></a></span>
                    }
                </h2>
            }
            <span class="bordered-icon"><i class="fa fa-circle-thin"></i></span>
        </div>
    </section>

    <div class="container">
        <div class="col-sm-10">
            @Html.Raw(ViewBag.Html)
        </div>
    </div>
    <!-- .container -->
</section>